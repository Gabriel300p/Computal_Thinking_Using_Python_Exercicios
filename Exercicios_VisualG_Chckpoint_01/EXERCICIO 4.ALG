Algoritmo "Exercicio 4"

Var
  numero1, numero2, divisao: real
inicio

//Entrada

escreva("Valor da gasolina: ")
leia(numero1)

escreva("Quanto deseja abastecer? ")
leia(numero2)

//Processamento

divisao := numero2 \ numero1


//Saida


escreva("Ira abastecer aproximadamente", divisao, " litros.")

Fimalgoritmo