Algoritmo "Exerciico 3"

Var
  numero1, numero2, soma,  resultado : inteiro
inicio

//Entrada

escreva("Valor do dolar: ")
leia(numero1)

escreva("Quantidade de Reais: ")
leia(numero2)


//Processamento

soma := numero2 \ numero1

resultado := soma

//Saida


escreva("Você tem", resultado, " em dolares")

Fimalgoritmo