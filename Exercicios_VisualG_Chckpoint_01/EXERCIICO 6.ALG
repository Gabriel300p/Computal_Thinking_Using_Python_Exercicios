Algoritmo "Exerciico 6"

Var
  numero1,  numero3, numero2, soma1, soma2 : real

inicio

//Entrada
escreva("Informe o valor de R: ")
leia(numero1)

escreva("Informe a altura: ")
leia(numero2)

numero3 <- 3.14


//Processamento

soma1 := numero1 * numero1
soma2 := numero3 * soma1 * numero2

//Saida


escreva("O volume cúbico é", soma2)

Fimalgoritmo