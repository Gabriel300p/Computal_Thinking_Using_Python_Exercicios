Algoritmo "Exercicio 5"

Var
  numero1,  numero3, numero2, soma1 : real

inicio

//Entrada
escreva("Digite a temperatura em celsius: ")
leia(numero1)

numero2 <- 1.8

numero3 <- 32


//Processamento

soma1 := numero1 * numero2 + numero3

//Saida


escreva("A temperatura é de", soma1 , " Farenheit")

Fimalgoritmo